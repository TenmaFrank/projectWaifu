<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/login_navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.tenmafrank.projectwife.ui.loginactivity.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_signinFragment"
            app:destination="@id/signinFragment" />
        <action
            android:id="@+id/action_loginFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/signinFragment"
        android:name="com.tenmafrank.projectwife.ui.loginactivity.login.signin.SigninFragment"
        android:label="SigninFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.tenmafrank.projectwife.ui.about.AboutFragment"
        android:label="AboutFragment" />
</navigation>